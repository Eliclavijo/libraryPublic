pipeline {
    agent any
  stages {
      stage('Checkout') {
            steps {
                script {
                    checkout scm
                }
            }
        }
    stage('SonarQube') {
      steps {
        echo "[STAGE] SonarQube"
        sh "sonar-scanner -Dsonar.projectKey=my-project -Dsonar.projectVersion=1.0.0 -Dsonar.sources=src/"
      }
    }
  }
}
